                {% for section in sections %}
                    <section class="section">
                        <h2 class="section-title">{{ section.title }}</h2>
                        <section id="{{ section.sectionkey }}">

                            {% if section.content.chronog %}
                                {% for exp in section.content.chronog %}
                                    <header>
                                        <h3 class="name">
                                            {{ exp.employer }}{% if exp.place %}, <span style="font-size:14px">{{ exp.place }}</span> {% endif %}
                                        </h3>
                                        <div class="date">
                                            {% set dates = exp.time.split(' -- ') %}
                                            {% if dates|length > 1 %}
                                                <time datetime="{{ dates[0] }}" class="startDate">
                                                    {{ dates[0] }}
                                                </time>
                                                –
                                                <time datetime="{{ dates[1] }}" class="endDate">
                                                    {{ dates[1] }}
                                                </time>
                                            {% else %}
                                                {{ dates[0] }}
                                            {% endif %}
                                        </div>
                                    </header>
                                    <div class="item">
                                        <div class="position">
                                            {{ exp.title }}
                                        </div>
                                        {% if exp.optshort | regex_search('\[(.*?)\]') is none %}
                                            {% if exp.website is defined %}
                                                <div class="website">
                                                   {{ exp.website }}
                                                </div>
                                            {% endif %}
                                            <div class="summary">
                                                <p>{{ exp.optshort }}</p>
                                            </div>
                                            <ul class="highlights">
                                                {{ exp.optlong }}
                                            </ul>
                                        {% else %}
                                            <div class="website">
                                                <a href="{{ exp.optshort | regex_search('\[(.*?)\]') }}">{{ exp.optshort | regex_search('\((.*?)\)') }}</a>
                                            </div>
                                            <div class="summary">
                                                <p>{{ exp.optlong }}</p>
                                            </div>
                                        {% endif %}
                                    </div>
                                {% endfor %}

                            {% elif section.content.lst %}
                                {% set design = namespace(design="default") %}
                                {% for val in section.content.lst %}
                                    {% if val.startswith('<!--design:') %}
                                        {%  set design.design = val[11:-3] %}
                                    {% endif %}
                                {% endfor %}
                                {% set design = design.design.strip() %}

                                {% if design == "default" %}
                                    <div class="listitems"><ul>
                                    {% for val in section.content.lst %}
                                        {% if not val.startswith('<!--design:') %}
                                            <li class=noinline>
                                                {{ val }}
                                            </li>
                                        {% endif %}
                                    {% endfor %}
                                    </ul></div>

                                {% elif design == "paragraphs" %}
                                    {% for val in section.content.lst %}
                                        {% if not val.startswith('<!--design:') %}
                                            <p style="margin-bottom:5px">{{ val }}</p>
                                        {% endif %}
                                    {% endfor %}

                                {% endif %}

                            {% elif section.content.basic %}
                                {% if section.content.design is defined %}
                                    {% set design = section.content.design %}
                                {% else %}
                                    {% set design = "default" %}
                                {% endif %}

                                {% if design == "default" %}
                                    {% set itembase = section.content.basic %}
                                {% elif design == "doubleitem" %}
                                    {% for key,val in section.content.basic.items() %}
                                        <div class="doubleitem">
                                            <h3 class="name">
                                                {{ key }}
                                            </h3>
                                            <div>
                                                <table><tr>
                                                    <td class="leftitem">{{ val[0] }}</td>
                                                    <td class="rightitem">{{ val[1] }}</td>
                                                </tr></table>
                                            </div>
                                        </div>
                                    {% endfor %}
                                {% elif design == "yearlist" %}
                                    <div class="yearlist">
                                        {% for key,val in section.content.basic.items() %}
                                          <header>
                                              <h3 class="name"><ul><li>{{ val }}</li></ul></h3>
                                              <div class="date">{{ key }}</div>
                                          </header>
                                        {% endfor %}
                                    </div>
                                {% elif design == "yearlist2" %}
                                    <div class="yearlist2">
                                        <table>
                                            {% for key,val in section.content.basic.items() %}
                                              <tr>
                                                <td class="">
                                                    <h3 class="name">{{ key }}</h3>
                                                </td>
                                                <td class="">
                                                    {{ val }}
                                                </td>
                                              </tr>
                                            {% endfor %}
                                        </table>
                                    </div>
                                {% endif %}
                            {% else %}
                                {% set itembase = section.content %}
                            {% endif %}
                            {% if itembase is defined %}

                                {% for key,val in itembase.items() %}
                                    <div class="item">
                                        <h3 class="name">
                                            {{ key }}
                                        </h3>
                                        <div style="font-style: italic;">
                                            {{ val }}
                                        </div>
                                    </div>
                                {% endfor %}

                            {% endif %}
                        </section>
                    </section>
                {% endfor %}

