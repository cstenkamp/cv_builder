        <script>
            function formatDate(input) {
              var date;
              if (typeof input === 'string') {
                var parts = input.split('/');
                if (parts.length === 2) { // "MM/YYYY" format, convert it to a Date object
                  date = new Date(parseInt(parts[1]), parseInt(parts[0]) - 1, 1);
                } else if (parts.length === 1) { // only year, just use that
                  return parts[0];
                } else {
                  console.error("Invalid input format");
                  return;
                }
              } else if (input instanceof Date) {
                date = input;
              } else {
                console.error("Invalid input format");
                return;
              }
              var monthNames = [
                "January", "February", "March",
                "April", "May", "June", "July",
                "August", "September", "October",
                "November", "December"
              ];

              var monthIndex = date.getMonth();
              var year = date.getFullYear();
              return ((monthIndex + 1) + "").padStart(2,"0") + '/' + year;
              // return year + '-' + (monthIndex + 1) + '-' + date.getDate(); // monthNames[monthIndex] + ' ' + year;
            }

            function formatdates() {
                var elems = document.getElementsByTagName("time");
                for (var i = 0; i < elems.length; i ++) {
                    elems[i].innerHTML = formatDate(elems[i].dateTime);
                }
            }
        </script>

